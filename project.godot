[application]

config/name="Caster"
config/description="The Caster Project, Andrometa team."
run/main_scene="res://Scenes/MainScenes/Menu/MainMenu.tscn"
config/icon="res://Assets/Textures/Andrometa/AndrometaLogoSmall.png"

[autoload]

GameData="*res://Scenes/Singletons/GameData.gd"
Globals="*res://Scenes/Singletons/Globals.gd"
Server="res://Scenes/Singletons/Server.gd"
Encryption="*res://Scenes/Singletons/Encryption.gd"
Utils="*res://Scenes/Singletons/Utils.gd"

[display]

window/size/width=800
window/size/fullscreen=true
window/size/test_width=1280
window/size/test_height=720
window/stretch/mode="viewport"
window/stretch/aspect="expand"
stretch/aspect="keep"
stretch/mode="2d"

